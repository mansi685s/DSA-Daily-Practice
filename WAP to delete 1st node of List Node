/*Problem- Wap to delete 1st Node of ListNode.
Level-Basic
Approach(Recursion)-
Return from 2nd element head.next;*/
//Solution-
class Node{
    int data;
    Node next;
    Node(int a){
        data=a;
    }
}

class Main {

    public static void print(Node head){
        while(head!=null){
            System.out.print(head.data+" ");
            head=head.next;
        }
    }

    public static Node update(Node head){
        if(head == null){
            return null;
        }
        return head.next;   // delete first node
    } 

    public static void main(String[] args) {
       Node n1=new Node(123456);
       Node n2=new Node(56789);
       Node n3=new Node(81223);
       Node n4=new Node(91023);

       n1.next=n2;
       n2.next=n3;
       n3.next=n4;

       Node x= update(n1);
       print(x);
    }
}
